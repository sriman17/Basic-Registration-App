<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>RegistrationApp</display-name>
  <display-name>RegistrationApp</display-name>
  <welcome-file-list>
    <welcome-file>Register.html</welcome-file>
  </welcome-file-list>
  
<servlet>
    <servlet-name>savestudent</servlet-name>
    <servlet-class>com.Registration.saveStudent</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>savestudent</servlet-name>
    <url-pattern>/savestudent</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>readstudent</servlet-name>
    <servlet-class>com.Registration.readStudent</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>readstudent</servlet-name>
    <url-pattern>/readstudent</url-pattern>
</servlet-mapping>
</web-app>